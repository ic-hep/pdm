# Demo application authentication

[groups/users]

[auth/users]
/users/api/v1.0/hello = "ALL"
/users/api/v1.0/users = "CERT"
# Note that this rule also matches the ones ending with turtles/<int:tid>
/users/api/v1.0/users%GET = "ANY"

/users/api/v1.0/get_token = "ALL"
/users/api/v1.0/verify_token = "TOKEN"

